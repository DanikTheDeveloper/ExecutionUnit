entity work.GPnet is
    Generic ( N : natural := 64 );
    Port (
        A, B : in std_logic_vector(N-1 downto 0);
        G, P    : out std_logic_vector(N-1 downto 0);
    );
end entity work.GPnet;

architecture Behaviour of work.GPnet is
begin
	gen_pg: for i in 0 to N-1 generate
        begin
            P(i) <= A(i) xor B(i);  -- Propagate signals
            G(i) <= A(i) and B(i);  -- Generate signals
        end generate;
end architecture Behaviour;